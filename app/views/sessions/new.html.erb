<% content_for(:page_title) do %>
Sign in!
<% end %>
<h1>Sessions#new</h1>

<%= form_tag do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= email_field_tag :email, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit_tag "Sign In", class: "btn btn-primary" %>
  </div>
<% end %>
